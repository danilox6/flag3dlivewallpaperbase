<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GalleryStyle">
        <attr name="android:galleryItemBackground" />
    </declare-styleable>

    <string-array name="pref_flag_image_entries">
        <item>Single</item>
        <item>Multiple</item>
    </string-array>
    <string-array name="pref_flag_image_values">
        <item>single_flag</item>
        <item>multi_flag</item>
    </string-array>
    <string-array name="pref_flag_mode_entries">
        <item>Fullscreen</item>
        <item>Sky</item>
    </string-array>
    <string-array name="pref_flag_mode_values">
        <item>flag_fullscreen</item>
        <item>flag_sky</item>
    </string-array>

    <declare-styleable name="SliderView">
        <attr name="multiplier" format="float" />
        <attr name="conversion" format="float" />
        <attr name="minValue" format="integer" />
        <attr name="maxValue" format="integer" />
        <attr name="value" format="integer" />
        <attr name="format" format="string" />
    </declare-styleable>

</resources>